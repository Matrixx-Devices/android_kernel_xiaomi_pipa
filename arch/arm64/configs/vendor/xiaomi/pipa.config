# Platform
CONFIG_MACH_XIAOMI_PIPA=y

# IR
CONFIG_IR_SPI=y

# Touchscreen
CONFIG_TOUCHSCREEN_NT36532_HOSTDL_SPI=y
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Power
CONFIG_QPNP_SMB5=n
CONFIG_QPNP_FG_GEN4=n
CONFIG_FUDA_1665=y
CONFIG_BATT_VERIFY_BY_DS28E16_PIPA=y
CONFIG_BQ2597X_CHARGE_PUMP_PIPA=y
CONFIG_DUAL_BQ2597X=y
CONFIG_ONEWIRE_GPIO_PIPA=y
CONFIG_QPNP_SMB5_PIPA=y
CONFIG_QPNP_FG_GEN4_PIPA=y

# Camera
CONFIG_TECHPACK_CAMERA_XIAOMI_TABLET=y

# Keyboard
CONFIG_KEYBOARD_NANO_8030_V2=y

# LLVM
CONFIG_LLVM_POLLY=y
CONFIG_INLINE_OPTIMIZATION=y
# CONFIG_THINLTO is not set
CONFIG_LTO_CLANG=y
CONFIG_LD_DEAD_CODE_DATA_ELIMINATION=y

# USB
CONFIG_PS5169=n

# KSU
CONFIG_KSU=y
CONFIG_OVERLAY_FS=y
# CONFIG_KSU_DEBUG is not set
# CONFIG_KSU_ALLOWLIST_WORKAROUND is not set
# CONFIG_KPROBES is not set
